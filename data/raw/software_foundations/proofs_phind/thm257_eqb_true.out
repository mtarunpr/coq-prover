Theorem eqb_true : forall n m,
  n =? m = true -> n = m.

Proof.
(*
     We will proceed by induction on the equality of n and m.
If n = m, then the statement is trivially true.
If n ≠ m, then we will show that n =? m must be false.
*)
  intros n m.
induction n as [|n' IHn'].
- (* n = 0 *)
    intros H.
(*
       We have n =? m = true and n = 0.
By the definition of =?, we know that m must also be 0.
Therefore, n = m.
*)
    discriminate H.
- (* n = S n' *)
    intros H.
(*
       We have n =? m = true and n = S n'.
By the definition of =?, we know that m must also be S m'.
Therefore, we have n' =? m' = true.
By the induction hypothesis, we know that n' = m'.
Therefore, S n' = S m'.
Hence, n = m.
*)
    discriminate H.
Qed.

