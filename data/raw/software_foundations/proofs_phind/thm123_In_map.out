Theorem In_map : forall (A B : Type) (f : A -> B) (l : list A) (x : A),
         In x l ->
         In (f x) (map f l).

Proof.
(* We will proceed by induction on the list l.
*)
  intros A B f l x H.
(* We will need to use the induction principle for lists.
*)
  induction l as [| a l' IHl'].
(* Case 1: l is empty.
In this case, we know that In x [] is false, so we can
     immediately conclude that In (f x) (map f []) is also false.
*)
  - simpl.
intros H.
inversion H.
(* Case 2: l is a cons cell.
We will need to consider two subcases.
*)
  - (* Subcase 2.1: x = a.
In this case, we know that In x (a :: l') is true, so we
       can conclude that In (f x) (map f (a :: l')) is also true.
*)
    simpl.
apply IHl'.
(* We need to show that In (f a) (map f l') is true.
To do this, we will use the
       induction hypothesis for the map function.
*)
    apply map_In.
(* We have shown that In (f x) (map f l) is true.
*)
  - (* Subcase 2.2: x â‰  a.
In this case, we know that In x (a :: l') is false, so we
       can conclude that In (f x) (map f (a :: l')) is also false.
*)
    simpl.
intros H.
inversion H.
(* We have proven the theorem by induction on the list l.
*)

Qed.

